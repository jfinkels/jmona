<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <bean id="crossoverFunction" class="jmona.example.monalisa.MonaCrossoverFunction" />
  
  <bean id="fitnessFunction" class="jmona.example.monalisa.MonaFitnessFunction">
    <property name="targetImage" ref="targetImage" />
  </bean>
  
  <bean id="mutatorFunction" class="jmona.example.monalisa.MonaMutatorFunction">
    <property name="height" ref="height" />
    <property name="width" ref="width" />
  </bean>

  <bean id="breedingFunction" class="jmona.impl.DefaultBreedingFunction">
    <property name="crossoverFunction" ref="crossoverFunction" />
  </bean>
  
  <bean id="selectionFunction" class="jmona.impl.MaxSelectionFunction" />

</beans>